public class ResearchExpedition {
    private ExpeditionMember expeditionLeader;
    private ExpeditionMember archivist;
    private ExpeditionMember feildResearcher;
    public ResearchExpedition(ExpeditionMember expeditionLeader, ExpeditionMember archivist, ExpeditionMember feildResearcher) {
        this.expeditionLeader = expeditionLeader;
        this.archivist = archivist;
        this.feildResearcher = feildResearcher;
    }
    public String getExpeditionLeader() {
        return expeditionLeader.getName();
    }
    public String getArchivist() {
        return archivist.getName();
    }
    public String getFeildResearcher() {
        return feildResearcher.getName();
    }
}
